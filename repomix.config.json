{
  "output": {
    "filePath": "aider-documentation.md",
    "style": "markdown",
    "parsableStyle": true,
    "fileSummary": true,
    "directoryStructure": true,
    "removeComments": true,
    "removeEmptyLines": true,
    "compress": true,
    "topFilesLength": 5,
    "showLineNumbers": false,
    "copyToClipboard": false
  },
  "include": [
    "aider/website/docs/usage.md",
    "aider/website/docs/install.md",
    "aider/website/docs/config.md",
    "aider/website/docs/faq.md",
    "aider/website/docs/git.md",
    "aider/website/docs/llms.md",
    "aider/website/docs/languages.md",
    "aider/website/docs/usage/**/*.md",
    "aider/website/docs/troubleshooting/**/*.md",
    "aider/website/docs/config/**/*.md",
    "aider/website/docs/install/**/*.md",
    "aider/website/docs/llms/**/*.md",
    "aider/README.md",
    "aider/CHANGELOG.md",
    "aider/CONTRIBUTING.md"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "**/*.svg",
      "**/*.jpg",
      "**/*.jpeg",
      "**/*.png",
      "**/*.gif",
      "**/LICENSE*",
      "**/node_modules/**",
      "**/venv/**",
      "**/.git/**",
      "**/tests/**",
      "**/test/**",
      "**/*_test.py",
      "**/*_tests.py",
      "**/test_*.py",
      "aider/website/**/*.html",
      "aider/website/**/*.yml",
      "aider/website/**/*.json",
      "aider/website/**/*.css",
      "aider/website/**/*.js",
      "aider/website/**/*.scss",
      "aider/website/**/*.ps1",
      "aider/website/**/*.sh",
      "aider/website/_*",
      "aider/website/assets/**",
      "aider/website/blog/**",
      "aider/website/_posts/**",
      "aider/website/_sass/**",
      "aider/website/_includes/**",
      "aider/website/_layouts/**",
      "aider/website/_data/**",
      "aider/website/examples/**",
      "aider/website/share/**",
      "aider/website/docs/recordings/**",
      "aider/website/docs/leaderboards/**",
      "aider/website/docs/legal/**",
      "aider/website/docs/more/**",
      "aider/website/docs/repomap.md",
      "aider/website/docs/scripting.md",
      "aider/website/docs/unified-diffs.md",
      "aider/website/docs/benchmarks*.md",
      "aider/website/docs/ctags.md",
      "aider/website/docs/more-info.md",
      "**/HISTORY.md"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}