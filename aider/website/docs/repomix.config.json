{
  "output": {
    "filePath": "aider-documentation.md",
    "style": "markdown",
    "parsableStyle": true,
    "fileSummary": true,
    "directoryStructure": true,
    "removeComments": true,
    "removeEmptyLines": true,
    "compress": true,
    "topFilesLength": 5,
    "showLineNumbers": false,
    "copyToClipboard": false
  },
  "include": [
    "usage.md",
    "install.md",
    "config.md",
    "faq.md",
    "git.md",
    "llms.md",
    "languages.md",
    "usage/**/*.md",
    "troubleshooting/**/*.md",
    "config/**/*.md",
    "install/**/*.md",
    "llms/**/*.md",
    "../../README.md",
    "../../CHANGELOG.md",
    "../../CONTRIBUTING.md"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "**/*.svg",
      "**/*.jpg",
      "**/*.jpeg",
      "**/*.png",
      "**/*.gif",
      "**/LICENSE*",
      "**/node_modules/**",
      "**/venv/**",
      "**/.git/**",
      "**/tests/**",
      "**/test/**",
      "**/*_test.py",
      "**/*_tests.py",
      "**/test_*.py",
      "../**/*.html",
      "../**/*.yml",
      "../**/*.json",
      "../**/*.css",
      "../**/*.js",
      "../**/*.scss",
      "../**/*.ps1",
      "../**/*.sh",
      "../_*",
      "../assets/**",
      "../blog/**",
      "../_posts/**",
      "../_sass/**",
      "../_includes/**",
      "../_layouts/**",
      "../_data/**",
      "../examples/**",
      "../share/**",
      "recordings/**",
      "leaderboards/**",
      "legal/**",
      "more/**",
      "repomap.md",
      "scripting.md",
      "unified-diffs.md",
      "benchmarks*.md",
      "ctags.md",
      "more-info.md",
      "**/HISTORY.md"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}